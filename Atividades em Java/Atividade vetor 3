import java.util.Scanner; // Biblioteca Scanner

public class Main {
  public static void main(String[] args) {
  
	Scanner S = new Scanner(System.in); 
    
 int [] N = new int[5]; // Variavel
 
 
 
 System.out.print("digite cinco numeros inteiros: "); 
 for (int i=0; i < 5; i++) { 
     N[i] = S.nextInt(); // armazena o numero 
     
     
     
   
     }
    
    System.out.println(" inversao desses numeros Ã© " + N[4] + "," +N[3]+ ","+ N[2]+","+ N[1]+ ","+ N[0]);
  
  }
}